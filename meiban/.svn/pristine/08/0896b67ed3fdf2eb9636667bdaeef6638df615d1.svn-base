	public String getURL() 
	{
		String url=null;
		try{
		    SAXBuilder builder = new SAXBuilder();   
		     Document doc = builder.build(new File("src/SetEnvironment.xml"));   
		     Element foo = doc.getRootElement();   
		     List allChildren = foo.getChildren();   
		    for (int i = 0; i < allChildren.size(); i++) {   
		    	url=((Element) allChildren.get(i)).getChild("url").getText();
	     //  System.out.print("URL:"+ ((Element) allChildren.get(i)).getChild("url").getText());   
		  //    System.out.println("³µÖ÷µØÖ·:"+ ((Element) allChildren.get(i)).getChild("ADDR").getText());   
		     }   
		    }
	      catch (Exception e) {   
		     e.printStackTrace();   
		   }   
		
		return url;
		 
		 
	       
	}
